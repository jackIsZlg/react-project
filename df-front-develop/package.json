    "dev": "egg-bin dev",
    "build": "npm i --production && tar  --exclude=.git  --exclude=.gitignore --exclude=.DS_Store --exclude=app/view/node_modules --exclude=app/view/common --exclude=app/view/components -zcvf release.tgz .",
    "devbuild": "npm i --production && tar  --exclude=.git  --exclude=.gitignore --exclude=.DS_Store --exclude=app/view/node_modules --exclude=app/view/common --exclude=app/view/components -zcvf release.tgz . && npm run devscp",
    "scp": "scp -r release.tgz root@47.96.100.221:/root/web",
    "devscp": "scp -r release.tgz root@47.96.100.221:/root/dev-web",
    "test": "npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "start": "npm run stop && egg-scripts start --port=8000 --daemon --title=pre",
    "prostart": "npm run prostop && egg-scripts start --port=8000 --daemon",
    "devstart": "npm run devstop && egg-scripts start --port=7000 --daemon --title=dev",
    "stop": "egg-scripts stop --title=pre",
    "prostop": "egg-scripts stop",
    "devstop": "egg-scripts stop --title=dev"
