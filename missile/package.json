{
  "name": "missile-node",
  "group": "com.wacai.client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run server & npm run dev",
    "start-test": "npm run dev & NODE_ENV=test npm start",
    "server": "nodemon --inspect -w config -w server -x babel-node server/server.js",
    "dev": "nodemon -w config -w scripts -w mock/mock-map scripts/server.js",
    "lint": "eslint . --fix --format codeframe",
    "lint-staged": "lint-staged",
    "test": "NODE_ENV=jest-test jest --forceExit",
    "precommit": "lint-staged",
    "check": "npm-check --update --skip-unused",
    "compile": "node scripts/compile.js",
    "deploy": "node scripts/deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.caimi-inc.com:client/missile.git"
  },
  "license": "MIT",
  "engines": {
    "node": "6.10.1",
    "pm2": "2.1.5"
  },
  "jest": {
    "verbose": true,
    "notify": true,
    "collectCoverage": true,
    "roots": [
      "<rootDir>/__tests__/page"
    ],
    "modulePaths": [
      "<rootDir>/app/page",
      "<rootDir>/app"
    ],
    "moduleDirectories": [
      "<rootDir>/__mocks__",
      "node_modules"
    ],
    "globals":{
      "__config__":{
        "baseURI": ""
      }
    },
    "coverageReporters": [
      "html",
      "text",
      "text-summary"
    ]
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "@wac/koa-logger": "^1.2.1",
    "@wac/papaya-api-map": "2.0.0",
    "@wac/papaya-auth": "2.0.0",
    "@wac/papaya-cas": "1.2.0",
    "@wac/papaya-proxy": "1.5.0",
    "@wac/papaya-request": "1.4.1",
    "@wac/papaya-ui": "1.0.30",
    "antd": "^2.13.2",
    "app-module-path": "^2.2.0",
    "axios": "^0.16.2",
    "babel-polyfill": "^6.9.0",
    "babel-runtime": "^6.9.0",
    "bluebird": "^3.4.1",
    "glob": "^7.0.5",
    "immutable": "^3.8.1",
    "jquery": "^1.12.4",
    "js-yaml": "^3.6.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-favicon": "^2.0.0",
    "koa-nunjucks-2": "^3.0.0",
    "koa-router": "^7.0.1",
    "koa-session-redis": "^1.1.0",
    "koa-static-cache": "^4.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.15.1",
    "nprogress": "^0.2.0",
    "qs": "^6.5.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.5",
    "react-router": "3.0.5",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^1.0.0",
    "redux-immutable": "^4.0.0",
    "redux-promise-middleware": "^4.3.0",
    "redux-thunk": "^2.1.0",
    "type-to-reducer": "^1.0.3"
  },
  "devDependencies": {
    "@wac/eslint-config-standard": "^2.0.0",
    "@wac/static-deploy": "^2.0.0",
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.11.1",
    "body-parser": "^1.15.1",
    "css-loader": "^0.28.0",
    "dev-ip": "^1.0.1",
    "dot-tpl-loader": "^1.2.1",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "execa": "^0.4.0",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "fs-extra": "^0.30.0",
    "happypack": "^3.0.3",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.17.0",
    "husky": "^0.13.3",
    "jest-cli": "^20.0.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^4.0.3",
    "lint-staged": "^3.4.0",
    "nodemon": "^1.9.2",
    "npm-check": "^5.2.2",
    "nunjucks": "^3.0.1",
    "nunjucks-loader": "^2.4.8",
    "postcss-loader": "^1.3.3",
    "style-loader": "^0.16.1",
    "supertest": "^2.0.0",
    "supertest-as-promised": "^4.0.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-plugin-hash-output": "^1.3.0"
  }
}
