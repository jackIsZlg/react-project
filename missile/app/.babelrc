{
  "presets": [
    ["es2015", { "modules": false}],
    ["react"]
  ],
  "env": {
    "jest-test": {
      "presets": [
        ["es2015"]
      ],
      "plugins": [
        "transform-object-rest-spread"
      ]
    }
  },
  "plugins": [
    "transform-runtime",
    ["import", { "libraryName": "antd" }],
    "syntax-dynamic-import",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "sourceMaps": true,
  "retainLines": true
}
